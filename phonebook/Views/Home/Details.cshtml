@model phonebook.Models.ContactBook
@{
    ViewBag.Title = Model.Name;
}
<h2>Контакт @Model.Name</h2>
<div>
    <dl class="dl-horizontal">
        <dt>Фамилия</dt>
        <dd>@Model.Soname</dd>

        <dt>Имя</dt>
        <dd>@Model.Name</dd>
        <dt>Отчество</dt>
        <dd>@Model.Midlename</dd>
        <dt>Дата рождения</dt>
        <dd>@Model.Data</dd>
        <dt>Телефон</dt>
        <dd>@Model.Phone</dd>
    </dl>
    <form asp-controller="Home" asp-action="Index" method="get">
        <div class="form-group">
            <input type="submit" class="btn btn-default" value="Назад" />
        </div>
    </form>
</div>